  ],
  "author": "Ruan Yulong <ruanyu1@gmail.com> (http://github.com/ruanyl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruanyl/tushare.js/issues"
  },
  "homepage": "https://github.com/ruanyl/tushare.js#readme",
  "dependencies": {
    "iconv-lite": "^0.4.15",
    "js-base64": "^2.1.9",
    "moment": "^2.17.1",
    "no-fetch": "^1.6.2",
    "whatwg-fetch": "^2.0.2",
    "strftime" : "^0.10.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
